{
  "name": "@johanlabs/maltese",
  "version": "1.1.0",
  "main": "index.js",
  "bin": {
    "maltese": "node ./index.js"
  },
  "scripts": {
    "test": "jest",

    "server": "node server.js",
    "server:dev": "nodemon server.js",

    "build": "npx babel src --out-dir dist",
    "build:extension": "npm run build && npm run update:extension",
    "update:extension": "npx terser dist/**/*.js --compress --mangle --keep-fnames --pure_getters --output extension/maltese.min.js"
  },
  "keywords": [
    "data-masking",
    "privacy",
    "security"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Johan Labs",
  "license": "ISC",
  "dependencies": {
    "ansi-escapes": "^7.0.0",
    "axios": "^1.7.9",
    "blessed": "^0.1.81",
    "cities.json": "^1.1.45",
    "cli-cursor": "^5.0.0",
    "cli-spinner": "^0.2.10",
    "cli-truncate": "^4.0.0",
    "commander": "^13.1.0",
    "compromise": "^14.14.4",
    "express": "^4.21.2",
    "faker": "^5.5.3",
    "inquirer": "^12.3.3",
    "lodash": "^4.17.21",
    "spinnies": "^0.5.1",
    "terminal-kit": "^3.1.2",
    "uuid": "^11.0.5",
    "validator": "^13.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  }
}
